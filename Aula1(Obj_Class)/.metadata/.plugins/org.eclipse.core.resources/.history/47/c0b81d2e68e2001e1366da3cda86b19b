package view;

import model.ContaBancaria;

public class Principal {

	public static void main(String[] args) {

		ContaBancaria conta = new ContaBancaria();
		
		conta.nome = "Jeanne";
		conta.numeroConta = 345698;
		conta.saldo = 1200.00d;
		
		conta.realizaDeposito(450.00d);
		conta.realizaSaque(200.00d);
		conta.realizaDeposito(2.00d);
		conta.realizaSaque(1000.00d);
	}

}
